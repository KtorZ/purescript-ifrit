{
    "schema_in": {
        "name": "string",
        "age": "number",
        "is_alive": "boolean",
        "spells": [{
            "power": "number",
            "name": "string"
        }]
    },

    "pipeline_in": [
        {
            "@": "reduce",
            "#": {
                "@": "field",
                "=": "age"
            },
            "=": {
                "nb": {
                    "@": "sum",
                    "=": {
                        "@": "constant",
                        "=": 1
                    }
                }
            }
        }
    ],



    "schema_out": {
        "nb": "number"
    },

    "pipeline_out": [
        {
            "$group": {
                "_id": "$age",
                "nb": {
                    "$sum": 1
                }
            }
        }
    ]
}
